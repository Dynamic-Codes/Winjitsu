SCHEME_NAME     	= "VS Cursor 12.0 (early)"
pointer		= "pointer.cur"
help			= "help.cur"
work			= "work.ani"
busy			= "busy.ani"
cross			= "cross.cur"
text			= "text.cur"
hand			= "hand.cur"
unavailiable	= "unavailiable.cur"
vert			= "vert.cur"
horz			= "horz.cur"
dgn1			= "dgn1.cur"
dgn2			= "dgn2.cur"
move			= "move.cur"
alternate		= "alternate.cur"
link			= "link.cur"
pin			= "pin.cur"
person		= "person.cur"